<h2>Create Listing</h2>

<%= form_for(@listing) do |f| %>
  
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  
  <%= fields_for :landlords do |landlord_form| %>
  <div class="field">
    <%= landlord_form.label :email %><br />
    <%= landlord_form.email_field :email %>
  </div>
  <% end %>
   
  <div class="actions"> 
    <%= f.submit "Create Listing" %>
  </div>
  
<% end %>